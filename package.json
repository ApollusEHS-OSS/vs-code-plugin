{
	"name": "aesop",
	"displayName": "Aesop",
	"description": "An IDE-native suite for Storybook.",
	"version": "0.0.4",
	"publisher": "async-aesop",
	"repository": "https://github.com/Async-Aesop/aesop",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.aesopAwaken",
		"workspaceContains:**/node_modules/@storybook/core/dist/public/*{.html, .js}",
		"onCommand:extension.getStories",
		"onWebviewPanel:aesop-sb"
	],
	"main": "./dist/extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.aesopAwaken",
				"title": "Aesop Awaken"
			},
			{
				"command": "extension.tollTheHour",
				"title": "Toll the Hour"
			},
			{
				"command": "extension.getStories",
				"title": "Get Storybook Stories"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"webpack-prod": "webpack --mode production",
		"test-compile": "tsc -p ./",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/express": "^4.17.2",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.12.21",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"ts-loader": "^6.2.1",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2",
		"webpack": "^4.41.4",
		"webpack-cli": "^3.3.10"
	},
	"dependencies": {
		"express": "^4.17.1",
		"npm": "^6.13.4",
		"vsce": "^1.71.0"
	}
}
